{
  "name": "maps",
  "version": "0.0.1",
  "description": "basic maps app using React and Graph QL",
  "main": "./src/index.js",
  "scripts": {
    "start": "babel-node ./server.js",
    "production": "NODE_ENV=production & npm start",
    "test": "jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:update": "./node_modules/.bin/jest --updateSnapshot",
    "lint": "./node_modules/.bin/eslint --ignore-pattern test.* ./src ",
    "relay": "relay-compiler --src ./src --schema ./data/schema.graphql --extensions jsx",
    "update-schema": "babel-node ./data/updateSchema.js"
  },
  "author": "liminal18",
  "license": "ISC",
  "dependencies": {
    "es6-promise": "^4.1.0",
    "google-map-react": "^0.24.0",
    "graphql": "^0.9.6",
    "graphql-relay": "^0.5.1",
    "isomorphic-fetch": "https://registry.npmjs.org/isomorphic-fetch/-/isomorphic-fetch-2.2.1.tgz",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-relay": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-relay": "^1.0.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "compress": "^0.99.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express-graphql": "^0.6.4",
    "helmet": "^3.6.0",
    "jest": "^20.0.1",
    "react-test-renderer": "^15.5.4",
    "relay-compiler": "^1.0.0",
    "require-clean": "^0.1.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "integration"
    ]
  }
}
